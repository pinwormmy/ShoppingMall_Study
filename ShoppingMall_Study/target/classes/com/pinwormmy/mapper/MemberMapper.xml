<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.pinwormmy.mapper.MemberMapper">

	<insert id="submitSignUp" parameterType="com.pinwormmy.mall.dto.MemberDTO">
	
		insert into memberlist(userId, userPassword, userEmail)
			values(#{userId}, #{userPassword}, #{userEmail})
			
	</insert>
  
</mapper>